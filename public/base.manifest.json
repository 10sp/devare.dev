{
  "manifest_version": 3,
  "name": "devare.dev - developer news",
  "description": "All developer news in one tab",
  "version": "1.24.4",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "host_permissions": [
    "https://*.devare.dev/*"
  ],
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://devare.dev/*"
      ],
      "run_at": "document_start",
      "js": [
        "content.js"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "src/features/rssFinder/contentScript.js"
      ]
    }
  ],
  "background": {
    "service_worker": "src/features/rssFinder/backgroundScript.js"
  },
  "icons": {
    "16": "/logos/logo16.png",
    "32": "/logos/logo32.png",
    "48": "/logos/logo48.png",
    "128": "/logos/logo128.png"
  }
}