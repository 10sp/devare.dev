{
  "name": "Hackertab.dev - developer news",
  "description": "All developer news in one tab",
  "version": "1.15.3",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://*.hackertab.dev/*"],
  "icons": {
    "16": "/logos/logo16.png",
    "32": "/logos/logo32.png",
    "48": "/logos/logo48.png",
    "128": "/logos/logo128.png"
  }
}
